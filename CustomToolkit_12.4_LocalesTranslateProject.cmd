echo.-------------------------------------------------------------------------------

:Sorting2AscendingOrder
for /l %%i IN (1, 1, !ImageCount!) DO (
	for %%c IN (!ImageIndexNo!) DO (
		if %%i equ %%c (
			set "AscendingImageIndexNoOrder=!AscendingImageIndexNoOrder!,%%c"
		)
	)
