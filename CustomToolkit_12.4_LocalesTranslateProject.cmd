	for /f "tokens=2 delims=:" %%c in ('%DISM% /Get-ImageInfo /ImageFile:"!InstallImageFile!" /Index:%~1 ^| findstr.exe /I "Name"') do (
		if "%%b" neq "arm64" (
			if %~1 leq 9 echo.^|   %~1   ^| %%b  ^|%%c
			if %~1 gtr 9 if %~1 lss 99 echo.^|   %~1  ^| %%b  ^|%%c
			if %~1 gtr 99 echo.^|   %~1 ^| %%b  ^|%%c
		)
		if "%%b" equ "arm64" (
			if %~1 leq 9 echo.^|   %~1   ^| %%b^|%%c
			if %~1 gtr 9 if %~1 lss 99 echo.^|   %~1  ^| %%b^|%%c
			if %~1 gtr 99 echo.^|   %~1 ^| %%b^|%%c
		)
	)>>"%ImageInfo%"
)
goto :EOF
